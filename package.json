{
  "name": "@lenne.tech/nuxt-extensions",
  "version": "1.2.11",
  "description": "Reusable Nuxt 4 composables, components, and Better-Auth integration for lenne.tech projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lenneTech/nuxt-extensions.git"
  },
  "homepage": "https://github.com/lenneTech/nuxt-extensions#readme",
  "bugs": {
    "url": "https://github.com/lenneTech/nuxt-extensions/issues"
  },
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "license": "MIT",
  "packageManager": "pnpm@10.29.2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./lib": {
      "types": "./dist/runtime/lib/index.d.ts",
      "import": "./dist/runtime/lib/index.js"
    },
    "./testing": {
      "types": "./dist/runtime/testing/index.d.ts",
      "import": "./dist/runtime/testing/index.js"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ],
      "lib": [
        "./dist/runtime/lib/index.d.ts"
      ],
      "testing": [
        "./dist/runtime/testing/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "git config core.hooksPath .githooks",
    "prepack": "nuxt-module-build build",
    "c": "pnpm run check",
    "check": "pnpm audit && pnpm run format:check && pnpm run lint && pnpm test:types && pnpm test && pnpm run build",
    "check:fix": "pnpm install && pnpm audit --fix && pnpm run format && pnpm run lint:fix && pnpm test:types && pnpm test && pnpm run build",
    "check:naf": "pnpm install && pnpm run format && pnpm run lint:fix && pnpm test:types && pnpm test && pnpm run build",
    "cf": "pnpm run check:fix",
    "cnaf": "pnpm run check:naf",
    "dev": "pnpm dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "build": "nuxt-module-build build",
    "lint": "oxlint src/",
    "lint:fix": "oxlint --fix --fix-suggestions src/",
    "format": "oxfmt --write src/",
    "format:check": "oxfmt --check src/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "release": "pnpm format:check && pnpm lint && pnpm test:types && pnpm test && pnpm prepack"
  },
  "dependencies": {
    "@nuxt/kit": "4.3.0"
  },
  "peerDependencies": {
    "@better-auth/passkey": ">=1.0.0",
    "@playwright/test": ">=1.0.0",
    "better-auth": ">=1.0.0",
    "nuxt": ">=3.0.0",
    "tus-js-client": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "better-auth": {
      "optional": false
    },
    "@better-auth/passkey": {
      "optional": true
    },
    "@playwright/test": {
      "optional": true
    },
    "tus-js-client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@better-auth/passkey": "1.4.18",
    "@nuxt/devtools": "3.1.1",
    "@playwright/test": "1.58.1",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.3.0",
    "@nuxt/test-utils": "3.23.0",
    "@types/node": "25.2.0",
    "@vitest/coverage-v8": "4.0.18",
    "@vue/test-utils": "2.4.6",
    "better-auth": "1.4.18",
    "happy-dom": "20.5.0",
    "nuxt": "4.3.0",
    "oxfmt": "0.28.0",
    "oxlint": "1.43.0",
    "tus-js-client": "4.3.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vue-tsc": "3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt4",
    "vue3",
    "better-auth",
    "authentication",
    "passkey",
    "webauthn",
    "tus-upload",
    "file-upload",
    "lenne-tech"
  ]
}